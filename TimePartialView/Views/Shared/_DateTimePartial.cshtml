@model DateTime
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<div>
    <h2>DateTime Partial View</h2>
    <h3>Date</h3>
    <p>@Model.Day / @Model.Month / @Model.Year</p>
    <h3>Time</h3>
    <p>@Model.Hour : @Model.Minute: @Model.Second</p>
</div>